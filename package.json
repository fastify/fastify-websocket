  "devDependencies": {
    "@types/ws": "^7.2.4",
    "fastify": "^3.0.0-rc.1",
    "pre-commit": "^1.2.2",
    "snazzy": "^8.0.0",
    "standard": "^14.3.3",
    "tap": "^14.10.7",
    "tsd": "^0.11.0"
  },
  "dependencies": {
    "fastify-plugin": "^2.0.0",
    "find-my-way": "^3.0.0",
