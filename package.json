  "devDependencies": {
    "@types/ws": "^8.2.0",
    "fastify": "^3.11.0",
    "pre-commit": "^1.2.2",
    "snazzy": "^9.0.0",
    "split2": "^3.2.2",
    "standard": "^16.0.1",
    "tap": "^15.0.2",
    "tsd": "^0.18.0"
  },
  "dependencies": {
    "fastify-plugin": "^3.0.0",
    "ws": "^8.0.0"
